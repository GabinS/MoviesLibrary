<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:MoviesLibrary.ClientApp.ViewModels"
                    xmlns:local="clr-namespace:MoviesLibrary.ClientApp.Views">


    <DataTemplate DataType="{x:Type viewModels:ViewModelMovies}">
        <local:SearchMoviesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:ViewModelMyMovies}">
        <local:MyMoviesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:ViewModelSettings}">
        <StackPanel>
            <TextBlock Text="{Binding Title}"/>
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>